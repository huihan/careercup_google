    while()
    for(int i = 0 ; i < len ; i++){
      if(  )
    }
    
  }
}

public class unit{
  int num;
  int val;
  int index;
  public int setFirst(String str){
    int k = 1 ;
    this.val = str.charAt(0);
    while(k < str.length()){
      if(str.charAt(k) == val){
        k++;
        continue;
      }
      else
       break;
    }
    return k;
  }
}
